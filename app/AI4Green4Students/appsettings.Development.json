{
  "ConnectionStrings": {
    "Default": "Host=localhost;Username=postgres;Port=5432;Password=example;Database=ai4green4students",
    "AI4Green": "Host=localhost;Username=postgres;Port=5432;Password=example;Database=ai4green",
    "AzureStorage": "UseDevelopmentStorage=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Registration": {
    "UseRules": true,
    "AllowList": ["@nottingham.ac.uk"],
    "BlockList": ["*", "blocked@nottingham.ac.uk"]
