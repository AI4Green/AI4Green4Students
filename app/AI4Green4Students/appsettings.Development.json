    "Provider": "SMTP",
    "SmtpHost": "localhost",
    "SmtpPort": 1025,
    "SmtpUsername": "example",
    "SmtpPassword": "example",
    "SmtpSecureSocketEnum": 1
  },
  "OpenIDConnect": {
    "Authority": "http://localhost:8080/realms/ai4green4students",
    "ClientId": "client-name",
